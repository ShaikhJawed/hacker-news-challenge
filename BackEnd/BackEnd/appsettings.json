{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "BaseUrls": {
    "HackerNews": "https://hacker-news.firebaseio.com/v0/"
  },
  "Limits": {
    "TopStories": 200,
    "MaxPageSize": 20,
    "MinPageNumber": 1,
    "CacheExpiryMinutes": 5
  },
  "AllowedHosts": "*"
}
